<div class="container-fluid d-block text-center py-4">
  <div class="row no-gutters">
    <div class="col-12 no-gutters">
      <h2 class="text-white bg-dark" style="font-family: Quicksand"id="hola">
           Sergei's Summer Course
      </h2>
    </div>
  </div>
</div>
<div id="form">
  <p>Please create your  new account</p>
  <hr/>
  <form action="/signup" method="post">
    <div class="form-group">
      <label for="form_username">Username</label>
      <input id="form_username" type="text" name="username" />
    </div>
    <div class="form-group">
      <label for="form_email">Email</label>
      <input id="form_email" type="email" name="email" />
    </div>
    <div class="form-group">
      <label for="form_password">Password</label>
      <input id="form_password" type="password" name="password" />
    </div>
    <button class="btn btn-primary btn-block" type="submit" value="Submit">Sign Up</button>
  </form>
  <hr/>
  <button class="btn btn-success btn-block" type="submit" href="/signup" onClick='document.location.href="/signup"'>
    Create an account
  </button>
</div>
